{
  "name": "figma-practice",
  "version": "1.0.0",
  "description": "Figma practice",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:prod",
    "build:prod": "NODE_ENV=production webpack --config ./webpack-prod.config.js",
    "build:dev": "NODE_ENV=development webpack --config ./webpack-dev.config.js",
    "types": "npx tsc",
    "serve": "NODE_ENV=development webpack serve --config ./webpack-dev.config.js  --entry ./src/index.tsx",
    "serve:prod": "webpack serve --config ./webpack-prod.config.js  --entry ./src/index.tsx",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix ./",
    "check": "npm run types && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/centralToNowhere/Figma-practice.git"
  },
  "author": "RojkovD",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/centralToNowhere/Figma-practice/issues"
  },
  "homepage": "https://github.com/centralToNowhere/Figma-practice#readme",
  "dependencies": {
    "@babel/runtime": "^7.17.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.8",
    "postcss-loader": "^6.2.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  }
}
